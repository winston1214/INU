    "import os\n",
    "ls = os.listdir('samsung')\n",
    "df_list = []\n",
    "for i in ls:\n",
    "    data = pd.read_csv('samsung/'+i)\n",
    "    df_list.append(data)\n",
    "df = pd.concat(df_list).reset_index(drop=True)"
   ]
